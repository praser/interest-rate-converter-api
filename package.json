{
  "name": "interest-tax-conversor",
  "description": "API to deal with interest tax conversions",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/praser/interest-tax-conversor"
  },
  "main": "src/entry.ts",
  "scripts": {
    "dev": "nodemon --watch \"src/\" --exec \"ts-node src/entry.ts\" -e ts",
    "test": "jest --no-cache",
    "build": "tsc",
    "prod": "npm run build && node dist/entry.js",
    "readme": "node ./node_modules/.bin/node-readme"
  },
  "keywords": [
    "anbima",
    "finance",
    "investiments",
    "cetip",
    "tax",
    "rate",
    "fee"
  ],
  "author": "Rubens Praser JÃºnior",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "dotenv": "^7.0.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/supertest": "^2.0.7",
    "jest": "^24.5.0",
    "node-readme": "^0.1.9",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.4000"
  }
}
